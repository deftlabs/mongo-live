{
    "name": "Mongo Live",
    "description": "An extension that allows you to view live/real-time MongoDB stats via the REST API.",
    "version": "0.5.6",

    "app": { "launch": { "local_path": "main.html" } },

    "icons": { "16": "icon_16.png", "48": "icon_48.png", "128": "icon_128.png" },
    
    "background_page": "background.html",

    "permissions": [ "unlimitedStorage", "http://*/" ]
}
